{
    "version": "2.0.0",
    "tasks": [
      {
        "type": "shell",
        "label": "gcc build test",
        "command": "gcc",
        "args": ["-Wall", "-Werror", "-g3", "${workspaceFolder}/test.c", "-o", "${workspaceFolder}/build/test.exe"],
        "problemMatcher": ["$gcc"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }